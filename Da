 a = int(input("Введите первое число:")) 
 b = int(input('Введите второе число:')) 
 if a > b: 
 print("первое число максимальное, а второе минимальное") 
 else: 
 print("второе число максимальное, а первое минимальное") 
 
 
 a = int(input("ВВедите сторону квадрата:")) 
 b = int(input("Введите радиус")) 
 if a > b: 
 print("влезет") 
 else: 
 print("не влезет") 
 
 
 a = int(input("Введите радиус:")) 
 b = int(input("Введите сторону квадрата")) 
 if a > b: 
 print("квадрат поместится") 
 else: 
 print("квадрат не поместится") 
 
 
 
 a = int(input("Введите первое число:")) 
 b = int(input("Введите второе число:")) 
 if a > b: 
 print("первое число - большее") 
 else: 
 print("второе число - большее") 


 

 
 a = int(input("Введите первое число:")) 
 b = int(input("Введите второе число:")) 
 if a < b: 
 print("первое число - меньшее") 
 else: 
 print("второе число -меньшее") 
 
 
